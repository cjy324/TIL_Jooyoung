var counter;

for (counter = 3; counter <= 15; counter += 3) {
  turnLeft();
  var repeat_end = counter / 3;
  for (var count = 0; count < repeat_end; count++) {
      moveForward();
  }
  for (var count2 = 0; count2 < counter; count2++) {
      getNectar();
  }
}
